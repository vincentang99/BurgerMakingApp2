<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <string-array name="burgerType">
        <item>Beef</item>
        <item>Chicken</item>
        <item>Pork</item>
    </string-array>

    <style name="Login" parent="Theme.AppCompat.Light.NoActionBar"></style>
    <style name="Register" parent="Theme.AppCompat.Light.NoActionBar"></style>
    <style name="Splash" parent="Theme.AppCompat.Light.NoActionBar"></style>

</resources>
